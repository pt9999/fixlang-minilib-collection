    let test_find_range_desc_desc_ok = |lt_begin, lt_end, expected| (
        let actual = set.find_raw_range_descending(lt_begin, lt_end).to_array;
        assert_equal("raw_range", expected, actual)
    );

    test_find_range_desc_ok(3, 6, [5, 4, 3]);;
